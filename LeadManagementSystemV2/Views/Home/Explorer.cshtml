
@{
    ViewBag.Title = "Engineering Policies";
    Layout = "~/Views/Shared/_FrontendLayout.cshtml";
}
<script src="~/fileman/js/jquery-2.1.1.min.js"></script>

<div class="container-fluid spacer-padding mt-6">
    <div class="row">

        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@ViewBag.WebsiteURL/home">Home</a></li>
                <li class="breadcrumb-item active" aria-current="page">@ViewBag.type policies</li>
            </ol>
        </nav>

        <div class="col-xl-12  mb-3">
            <div class="card border-0 mb-3">
                <div class="card-header"><h3>Policies</h3></div>
                <div class="card-body">
                    <div class="row" style="height:600px">
                        <div class="col-md-12">
                            <iframe src="~/fileman/index.html" style="width:100%;height:500px" frameborder="0">
                            </iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@section InLineScriptSection{
    <script>
        $.when($.ready).done(function () {
            eraseCookie("keyword");
        });
    </script>
}